<idea-plugin>
    <id>org.jetbrains.extime</id>
    <name>Execution-time</name>
    <vendor email="lalalaSasha@yandex.ru" url="www.nety.com">Sasha</vendor>

    <description>simple plugin for counting runtime</description>
    <change-notes>first version</change-notes>
    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->
    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <project-components>
        <component>
            <implementation-class>ExecutionTime</implementation-class>
        </component>
    </project-components>
</idea-plugin>